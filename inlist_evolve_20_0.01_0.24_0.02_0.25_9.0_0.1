

&star_job
  show_log_description_at_start = .true.

  load_saved_model = .true.
  saved_model_name = "irrad_20_0.01_0.24_0.02_0.25_9.0.mod"

  save_model_when_terminate = .true.
  save_model_filename = "evolve_20_0.01_0.24_0.02_0.25_9.0_0.1.mod"
  
  write_profile_when_terminate = .true.
  filename_for_profile_when_terminate = "profile_evolve20_0.01_0.24_0.02_0.25_9.0_0.1"
  
  set_initial_model_number = .true.
  initial_model_number = 0       

  !set_initial_age = .true.
  !initial_age = 6000000.0

  eos_file_prefix = 'mesa'
  kappa_file_prefix = 'gs98'
  kappa_lowT_prefix = 'lowT_Freedman11'
  
  change_lnPgas_flag = .true.
  new_lnPgas_flag = .true.

  pgstar_flag = .false.
  !pause_before_terminate = .true. ! lets you examine the final pgstar window when run terminates

/ ! end of star_job namelist

&controls
  max_number_backups = 100
  max_number_retries = 500

  varcontrol_target = 9d-5
  mesh_delta_coeff = 0.5

  ! surface heating
  column_depth_for_irradiation = 6.784539615040002 ! 3.d2 cm^2/g
  irradiation_flux = 20357683.70173884! 1.d9 erg/cm^2/s ! day side flux!!! Bill puts in 1/4 by hand

  ! when to stop
  max_age = 1d10
  max_model_number = 10000 ! don't let it go on and on if run stuck
  Teff_lower_limit = -1d99
  T_mix_limit = 0
  logQ_limit=50000000000
      
  ! output to files and terminal
  photo_interval  = 10
  profile_interval = 10
  max_num_profile_models= 500
  history_interval = 1
  !terminal_cnt = 10
  write_header_frequency = 10

  x_ctrl(50)= 0.1                  !frac_absorbed_euv (Î·)
  x_ctrl(51)= 1.0                       !frac_absorbing_radius (a)
  x_ctrl(52)= 1.0                      !host_star_mass (this gets multiplied by msol)
  x_ctrl(53)= 0.1                      !escape_rate_reduction_factor
  x_ctrl(54)= 0.25                 !orbital_distance (this gets multipled by au)
  x_ctrl(55)= 1000000000.0                      !eddy coefficient
  x_ctrl(56)= 100			  !Only run the comp change if true 
  x_ctrl(57)= 517.6590664625793       !Only run the comp change if true 
	
  use_other_adjust_mdot = .true.
  use_other_energy = .true.
  x_ctrl(3) = 1d10 ! core heat capacity  eps=erg/g/K
  x_ctrl(4) = 1.0 ! fraction of core mass in "chondrite-like" rocky material

  star_history_name = "evolve_20_0.01_0.24_0.02_0.25_9.0_0.1.mod"

/ ! end of controls namelist
&pgstar
/ ! end of pgstar namelist
